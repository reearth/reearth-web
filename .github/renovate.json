{
  "extends": [
    "config:base",
    ":semanticCommits",
    ":semanticCommitScopeDisabled",
    ":maintainLockFilesWeekly",
    ":enableVulnerabilityAlertsWithLabel(security)",
    ":semanticCommitTypeAll(chore)"
  ],
  "rangeStrategy": "bump",
  "enabledManagers": [
    "npm"
  ],
  "lockFileMaintenance": {
    "automerge": true,
    "automergeType": "branch"
  },
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "excludePackagePatterns": [
        "cesium"
      ],
      "enabled": false
    }
  ]
}
